<%-include('../includes/head.ejs')%>
<link rel="stylesheet" href="../css/all.css">
</head>
<body>
<%-include('../includes/landlordnavigation.ejs')%>
<div class="conatiner">
    <div class="row inform-card">
        <div class="col-lg-3 col-md-6 col-10 mx-auto my-3">
            <div class="card">
                <div class="card-body">
                    <div class="flex d-flex justify-content-between">
                        <i class="fa fa-home body-icon"></i>
                        <div class="side-text align-self-right">
                            <h3>20 Registered</h3>
                            <h5>Tenents</h5>
                        </div>
                    </div>
                </div>

            </div>
        </div>
        <div class="col-lg-3 col-md-6 col-10 mx-auto my-3">
            <div class="card card-2">
                <div class="card-body">
                    <div class="flex d-flex justify-content-between">
                        <i class="fa fa-users body-icon"></i>
                        <div class="side-text align-self-center">
                            <h3>10 Empty</h3>
                            <h5>Houses</h5>
                        </div>
                    </div>
                </div>

            </div>
        </div>
        <div class="col-lg-3 col-md-6 col-10 mx-auto my-3">
            <div class="card card-4">
                <div class="card-body">
                    <div class="flex d-flex justify-content-between">
                        <i class="fa fa-suitcase  body-icon"></i>
                        <div class="side-text align-self-center">
                            <h3>20 House</h3>
                            <h5>Units</h5>
                        </div>
                    </div>
                </div>
            </div>
        </div>


    </div>
</div>
<div class="container">
    <%if(prods.length>0){%>
    <div class="row py-5">
        <%for(let product of prods){%>
            <div class="col-10 col-md-6 col-lg-4 mx-auto my-3">

                <div class="card">
                    <div class="img-container">
                        <img src="../images/apartment-one.png" alt="property image" class="card-img-top"/>
                        <div class="featured-icons">
                            <a href="#" class="featured-icon-left"> <i class="fas fa-chevron-left"></i> </a>
                            <a href="#" class="featured-icon-right"> <i class="fas fa-chevron-right"></i> </a>
                        </div>
                    </div>
                    <!-- card body -->
                    <div class="card-body">
                        <div class="card-title text-capitalize">
                            <h5 class="mb-3">excellent property in  <%=product.City%></h5>
                            <p> <span class="text-green"> <i class="fas fa-map-marker-alt"></i> </span>
                                <%=product.HouseName%> <%=product.Colony%> <%=product.Street%> , <%=product.District%> <%=product.Pincode%>
                            </p>
                        </div>
                    </div>
                    <div class="card-footer text-center" id="user-card-footer">
                        <a href="/user-details/<%= product._id %>" class="btn btn-primary mx-auto">Details</a>
                    </div>
                </div>
            </div>
        <%}%>
    </div>
    <%}else{%>
        <h1>NO produt Found!1</h1>
    <%}%>
</div>
<%-include('../includes/end.ejs')%>
